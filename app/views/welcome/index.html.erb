<div class="col-lg-1">
  <div class="feed_holder">
      <% if current_user %>
        <div>
          <%= image_tag current_user.image %>
          <h4><%= current_user.name %></h4>
        </div>
      <% end %>
      <% if @feed.nil? %>
        <div>
          <h3>Start searching and following to create a feed.</h3>
        </div>
      <% else %>
        <% @feed.each do |feed_item| %>
            <div class="narrower-holder">
              <%=raw feed_item.embed %>
            </div>
        <% end %>
      <% end %>
  </div>
</div>
