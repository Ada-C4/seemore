  <div class="feed_holder">
    <% if current_user %>
      <div class="user-pic-name">
        <div class="background-shade">
          <%= image_tag current_user.image, class: "heightlimits" %>
          <h4 class="name">Welcome,<br><%= current_user.name %></h4>
        </div>
      </div>
    <% end %>
    <% if @feed.nil? %>
      <div class="narrower-holder">
        <h3>Start searching and following to create a feed.</h3>
      </div>
    <% else %>
      <% @feed.each do |feed_item| %>
          <div class="narrower-holder">
            <%=raw feed_item.embed %>
          </div>
      <% end %>
    <% end %>
  </div>
